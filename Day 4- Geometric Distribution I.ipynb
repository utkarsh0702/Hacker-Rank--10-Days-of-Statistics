   "source": [
    "def geometric_distributon(n, p):\n",
    "    return ((1-p)**(n-1))*p\n",
    "\n",
    "a, b = list(map(int, input().split()))\n",
    "n = int(input())\n",
    "print('{:.3f}'.format(geometric_distributon(n, a/b)))"
   ]
  }
