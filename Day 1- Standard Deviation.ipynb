   "source": [
    "import math\n",
    "\n",
    "N= int(input())\n",
    "x= list(map(int, input().split()))\n",
    "\n",
    "mean= sum(x)/N\n",
    "for i in range(N):\n",
    "    x[i]= (x[i]- mean)**2\n",
    "\n",
    "std= math.sqrt(sum(x)/N)\n",
    "print(round(std,1))\n"
   ]
